  "scripts": {
    "postinstall": "npx husky install",
    "clean": "rm -rf ./packages/logic && mkdir -p ./packages/logic",
    "build": "npm run clean && cd ./packages/logic && python3 -m grpc_tools.protoc -I../protos --python_out=. --pyi_out=. --grpc_python_out=. ../protos/monitor.proto"
  },
