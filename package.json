    "email": "mortezajamali4241@gmail.com",
    "url": "https://moraj.ir"
  },
  "bugs": {
    "url": "https://github.com/morajlab-aprp/protos/issues"
  },
  "homepage": "https://github.com/morajlab-aprp/protos#readme",
